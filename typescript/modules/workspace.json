{
  "version": 2,
  "projects": {
    "angular-apps-app": {
      "projectType": "application",
      "root": "apps/angular/apps/app",
      "sourceRoot": "apps/angular/apps/app/src",
      "prefix": "allors",
      "targets": {
        "build": {
          "executor": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/angular/apps/app",
            "index": "apps/angular/apps/app/src/index.html",
            "main": "apps/angular/apps/app/src/main.ts",
            "polyfills": "apps/angular/apps/app/src/polyfills.ts",
            "tsConfig": "apps/angular/apps/app/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": ["apps/angular/apps/app/src/favicon.ico", "apps/angular/apps/app/src/assets"],
            "styles": ["./node_modules/@angular/material/prebuilt-themes/indigo-pink.css", "apps/angular/apps/app/src/styles.scss", "./node_modules/easymde/dist/easymde.min.css"],
            "scripts": [],
            "allowedCommonJsDependencies": ["zone.js", "markdown.component", "easymde", "codemirror", "codemirror-spell-checker"]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/angular/apps/app/src/environments/environment.ts",
                  "with": "apps/angular/apps/app/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "executor": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "angular-apps-app:build:production"
            },
            "development": {
              "browserTarget": "angular-apps-app:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "executor": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "angular-apps-app:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/angular/apps/app/src/**/*.ts", "apps/angular/apps/app/src/**/*.html"]
          },
          "outputs": ["{options.outputFile}"]
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/angular/apps/app"],
          "options": {
            "jestConfig": "apps/angular/apps/app/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
