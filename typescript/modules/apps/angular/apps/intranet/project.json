{
  "projectType": "application",
  "root": "apps/angular/apps/intranet",
  "sourceRoot": "apps/angular/apps/intranet/src",
  "prefix": "allors",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/angular/apps/intranet",
        "index": "apps/angular/apps/intranet/src/index.html",
        "main": "apps/angular/apps/intranet/src/main.ts",
        "polyfills": "apps/angular/apps/intranet/src/polyfills.ts",
        "tsConfig": "apps/angular/apps/intranet/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          "apps/angular/apps/intranet/src/favicon.ico",
          "apps/angular/apps/intranet/src/assets"
        ],
        "styles": ["./node_modules/@angular/material/prebuilt-themes/indigo-pink.css", "apps/angular/apps/intranet/src/styles.scss", "./node_modules/easymde/dist/easymde.min.css"],
        "scripts": [],
        "allowedCommonJsDependencies": ["zone.js", "markdown.component", "easymde", "codemirror", "codemirror-spell-checker"]
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "fileReplacements": [
            {
              "replace": "apps/angular/apps/intranet/src/environments/environment.ts",
              "with": "apps/angular/apps/intranet/src/environments/environment.prod.ts"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "browserTarget": "angular-apps-intranet:build:production",
          "proxyConfig": "apps/angular/apps/intranet/proxy.conf.json"
        },
        "development": {
          "browserTarget": "angular-apps-intranet:build:development",
          "proxyConfig": "apps/angular/apps/intranet/proxy.conf.json"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "angular-apps-intranet:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "apps/angular/apps/intranet/src/**/*.ts",
          "apps/angular/apps/intranet/src/**/*.html"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/angular/apps/intranet"],
      "options": {
        "jestConfig": "apps/angular/apps/intranet/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
