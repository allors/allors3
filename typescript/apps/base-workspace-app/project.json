{
  "name": "base-workspace-app",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/base-workspace-app/src",
  "prefix": "allors",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/base-workspace-app",
        "index": "apps/base-workspace-app/src/index.html",
        "main": "apps/base-workspace-app/src/main.ts",
        "polyfills": "apps/base-workspace-app/src/polyfills.ts",
        "tsConfig": "apps/base-workspace-app/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          "apps/base-workspace-app/src/favicon.ico",
          "apps/base-workspace-app/src/assets"
        ],
        "styles": [
          "apps/base-workspace-app/src/styles.scss",
          "./node_modules/easymde/dist/easymde.min.css"
        ],
        "scripts": [],
        "allowedCommonJsDependencies": [
          "easymde",
          "codemirror",
          "codemirror-spell-checker"
        ]
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "2.5mb",
              "maximumError": "3mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "fileReplacements": [
            {
              "replace": "apps/base-workspace-app/src/environments/environment.ts",
              "with": "apps/base-workspace-app/src/environments/environment.prod.ts"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "browserTarget": "base-workspace-app:build:production",
          "proxyConfig": "apps/base-workspace-app/proxy.conf.json"
        },
        "development": {
          "browserTarget": "base-workspace-app:build:development",
          "proxyConfig": "apps/base-workspace-app/proxy.conf.json"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "base-workspace-app:build"
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "apps/base-workspace-app/**/*.ts",
          "apps/base-workspace-app/**/*.html"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/apps/base-workspace-app"],
      "options": {
        "jestConfig": "apps/base-workspace-app/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
