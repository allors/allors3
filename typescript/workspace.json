{
  "version": 2,
  "projects": {
    "angular-base-app": {
      "projectType": "application",
      "root": "apps/angular/base/app",
      "sourceRoot": "apps/angular/base/app/src",
      "prefix": "allors",
      "targets": {
        "build": {
          "executor": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/angular/base/app",
            "index": "apps/angular/base/app/src/index.html",
            "main": "apps/angular/base/app/src/main.ts",
            "polyfills": "apps/angular/base/app/src/polyfills.ts",
            "tsConfig": "apps/angular/base/app/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": ["apps/angular/base/app/src/favicon.ico", "apps/angular/base/app/src/assets"],
            "styles": ["apps/angular/base/app/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/angular/base/app/src/environments/environment.ts",
                  "with": "apps/angular/base/app/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "executor": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "angular-base-app:build:production"
            },
            "development": {
              "browserTarget": "angular-base-app:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "executor": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "angular-base-app:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/angular/base/app/src/**/*.ts", "apps/angular/base/app/src/**/*.html"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/angular/base/app"],
          "options": {
            "jestConfig": "apps/angular/base/app/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
